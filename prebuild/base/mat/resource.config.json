                },
                {
                    "type": "?",
                    "exports": {
                        "imageFile": "prebuild/base/token/resource/relics.jpg",
                        "tags": ["resource.relics"]
                    },
                    "cloneId": "resource"
                },
                {
                    "type": "?",
                    "exports": {
                        "imageFile": "prebuild/base/token/resource/psionic.jpg",
